{%
set classes = [
'node',
'node--type-' ~ node.bundle|clean_class,
node.isPromoted() ? 'node--promoted',
node.isSticky() ? 'node--sticky',
not node.isPublished() ? 'node--unpublished',
view_mode ? 'node--view-mode-' ~ view_mode|clean_class,
'clearfix',
]
%}


<article{{ attributes.addClass(classes)|without('about') }}>
<!--*********************************************| HEAD |****************************************************-->
    <div class="example__head">
        <div></div>
    </div>
<!--*********************************************| BODY |****************************************************-->
    <div class="example__body">
        <!--(*******************************) body middle left (************************)-->
        <div class="example__body_left">
        </div>
        <!--(*******************************) body middle center (************************)-->
        <div class="example__body_center">
            <div class="example__body_center_top"></div>
            <div class="example__body_center_middle"></div>
            <div class="example__body_center_bottom"></div>
        </div>
        <!--(*******************************) body middle right (************************)-->
        <div class="example__body_right">
        </div>
    </div>
<!--*********************************************| FOOT |****************************************************-->
    <div class="example_foot">

    </div>
<!--*********************************************| END example |****************************************************-->
</article>
{#<header>#}
        {#{{ title_prefix }}#}
        {#{% if not page %}#}
            {#<h2{{ title_attributes.addClass('node__title') }}>#}
                {#<a href="{{ url }}" rel="bookmark">{{ label }}</a>#}
            {#</h2>#}
        {#{% endif %}#}
        {#{% set title_suffix = '' %}#}
        {#{{ title_suffix }}#}
        {#{% if display_submitted %}#}
            {#<div class="node__meta">#}
                {#{{ author_picture }}#}
                {#<span{{ author_attributes }}>#}
          {#{% trans %}Submitted by {{ author_name }} on {{ date }}{% endtrans %}#}
        {#</span>#}
                {#{{ metadata }}#}
            {#</div>#}
        {#{% endif %}#}
    {#</header>#}
    {#<div{{ content_attributes.addClass('node__content', 'clearfix') }}>#}
        {#{{ content }}#}
    {#</div>#}

